{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "paths": {
      "~components": ["./src/components"],
      "~constants": ["./src/constants"],
      "~types": ["./src/types"],
      "~api": ["./src/api"],
      "~assets/*": ["./assets/*"],
      "~screens/*": ["./src/screens/*"],
      "~navigators/*": ["./src/navigators/*"],
      "~stores/*": ["./src/stores/*"],
      "~utils/*": ["./src/utils/*"]
    },
    "module": "commonjs"
  }
}
